	"description": "Menu Management module for manage menu in sidebar dashboard. Build on top of nwidart/laravel-modules",
	"type": "laravel-module",
	"license": "MIT",
	"authors": [
		{
			"name": "Vicky Rahman",
			"email": "pratamavr@gmail.com"
		}
	],
	"require": {
		"php": "^8.2",
		"laravel/framework": "^12.0",
		"nwidart/laravel-modules": "^5.0",
		"joshbrw/laravel-module-installer": "^1.0",
		"vicky-project/viewmanager-module": "^1.0"
	},
	"extra": {
		"laravel": {
			"providers": [],
			"aliases": {}
		}
